package test.java;

import org.testng.annotations.Test;

public class helloworld {

@Test
public void sample() {
	
	System.out.println("hi good morning");
	
	
		}

@Test
public void Fibonacci() {
	
	int n = 10, t1 = 0, t2 = 1;
    System.out.print("First " + n + " terms: ");

    for (int i = 1; i <= n; ++i)
    {
        System.out.print(t1 + " + ");

        int sum = t1 + t2;
        t1 = t2;
        t2 = sum;
    }
    //System.out.println("hi good morning);
	
	
		}


@Test
public void test() {
	
	System.out.println("Have Breakfast");
	
	
		}

