package test.java;

import org.testng.annotations.Test;

@Test
public class leapyear 
{
	int year = 2016;
    System.out.println();        
    if((year % 400 == 0) || ((year % 4 == 0) && (year % 100 != 0)))
            System.out.println("Year " + year + " is a leap year");
    else
            System.out.println("Year " + year + " is not a leap year");
            System.out.println();

}
}
